{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": " {{ .Folder }} :: {{if .Root}}Admin{{end}}",
  "palette": {
    "main-bg": "#24283b",
    "terminal-blue": "#7aa2f7",
    "terminal-green": "#73daca",
    "terminal-magenta": "#bb9af7",
    "terminal-red": "#f7768e",
    "terminal-white": "#c0caf5",
    "terminal-yellow": "#e0af68",
    "t-background": "p:main-bg",
    "aws-pink": "#ff3a7f",
    "blue-bell": "#9aa5ce",
    "celeste-blue": "#b4f9f8",
    "docker-blue": "#1d9bf5",
    "invader-green": "#34ef8e",
    "light-sky-blue": "#7dcfff",
    "node-mantis": "#6cc24a",
    "postgres-blue": "#336791",
    "pumpkin-orange": "#ff9513",
    "python-mustard": "#ffde57",
    "ruby-red": "#e0115f",
    "terraform-violet": "#a851ff",
    "white-blue": "#a9b1d6"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "background": "transparent",
          "foreground": "p:terminal-blue",
          "template": "\ue711 "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "p:invader-green",
          "options": {
            "style": "folder"
          },
          "template": "<b>\uea83 {{ .Path }}</b> <p:terminal-magenta>\uea84 </>"
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:light-sky-blue",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:terminal-red{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0)}}p:light-sky-blue {{ end }}",
            "{{ if gt .Ahead 0 }}p:terminal-blue{{ end }}",
            "{{ if gt .Behind 0 }}p:celeste-blue{{ end }}"
          ],
          "template": "({{ .HEAD}})",
          "options": {
            "fetch_status": true,
            "branch_icon": "\ue725 "
          }
        },
        {
          "type": "status",
          "style": "plain",
          "foreground": "p:terminal-red",
          "template": " \uf00d"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "type": "aws",
          "style": "plain",
          "foreground": "p:aws-pink",
          "template": "\uf0ef {{ .Profile}}{{if .Region}}@{{.Region}}{{end}} "
        },
        {
          "type": "command",
          "style": "plain",
          "foreground": "p:terraform-violet",
          "options": {
            "shell": "bash",
            "command": "asdf current terraform | awk 'NR==2 {print $2}'"
          },
          "template": "\ue69a {{ .Output }} "
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:node-mantis",
          "template": "\ue718 {{ .Full }} "
        },
        {
          "type": "command",
          "style": "plain",
          "foreground": "p:python-mustard",
          "options": {
            "shell": "bash",
            "command": "python --version | sed 's/Python //'"
          },
          "template": "\ue606 {{ .Output }} "
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "p:light-sky-blue",
          "template": "{{ if .Full }}\ue627 {{ .Full }} {{ end }}"
        },
        {
          "type": "ruby",
          "style": "plain",
          "foreground": "p:ruby-red",
          "options": {
            "shell": "bash",
            "command": "asdf current ruby | awk 'NR==2 {print \"v\"$2}'"
          },
          "template": "\uE791 {{ .Output }} "
        },
        {
          "type": "kubectl",
          "style": "plain",
          "foreground": "p:terminal-blue",
          "template": "\u2388 {{ .Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} "
        },
        {
          "foreground": "p:white-blue",
          "options": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ueba2 {{ .FormattedMs }}",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:terminal-yellow",
          "style": "plain",
          "template": "\ueb6d",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "p:terminal-blue",
    "template": "\u279c "
  },
  "transient_prompt": {
    "background": "p:t-background",
    "foreground": "p:terminal-blue",
    "template": "\u279c "
  },
  "final_space": true,
  "version": 2,
  "terminal_background": "p:t-background"
}
